class Solution(object):
    def generate(self, numRows):
        triangle = []
        triangle.append([1])
        for i in range(1,numRows):
            prev = triangle[i-1]
            new = [1]
            for j in range(1,len(prev)):
                new.append(prev[j-1] + prev[j])
            new.append(1)
            triangle.append(new)
        return triangle


        
